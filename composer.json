{
	"name": "stoic/pdo",
	"description": "PDO helper components for the Stoic framework",
	"version": "1.3.2",
	"homepage": "https://stoic-framework.com",
	"autoload": {
		"classmap": ["Pdo/"],
		"exclude-from-classmap": [
			"./Tests/"
		]
	},
	"repositories": [
		{
			"url": "https://github.com/AndyM84/pseudo.git",
			"type": "git"
		}
	],
	"require": {
		"php": ">=8.3",
		"stoic/stoic": "^1",
		"stoic/io": "^1",
		"ext-pdo": "*",
		"ext-json": "*"
	},
	"require-dev": {
		"phpunit/phpunit": "^9",
		"jimbojsb/pseudo": "master-dev"
	}
}
